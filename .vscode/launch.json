    "version": "0.2.0",
    "configurations": [
        {
            "type": "bashdb",
            "request": "launch",
            "name": "Bash-Debug",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/builder/linux-static",
            "args": ["build-release"]
        },
        {
            "name": "Stagenet Miner",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/builder/build/debug/bin/slated",
            "args": [
                "--stagenet",
                "--data-dir", "~/.slate/stagenet/node4",
                "--no-igd",
                "--hide-my-port",
                "--p2p-bind-port", "58080",
                "--rpc-bind-port", "58081",
                "--zmq-rpc-bind-port", "58082",
                "--rpc-restricted-bind-port", "58083",
                "--add-exclusive-node", "127.0.0.1:28080",
                "--add-exclusive-node", "127.0.0.1:38080",
                "--add-exclusive-node", "127.0.0.1:48080",
                "--start-mining", "SL2FeC93PLC7qpGPFcg89o7RCDGdF8KGC2ZHzGziwq2SR51AxRnV8sqLrLAupQftSPMn7DAKxENpsdzJE8nJ6igi31Yp2BeKg",
                "--mining-threads", "1",
                "--block-sync-size", "1"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Stagenet",
